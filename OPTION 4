#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    double x,y,z;

    cout<<"x ="<<'\n';
    cin>>x;

    cout<<"y ="<<'\n';
    cin>>y;

    cout<<"z ="<<'\n';
    cin>>z;

    double s1 = fabs(cos(x) - cos(y));
    double s2 = 1.0 + 2 * pow(sin(y),2);
    double s3 = 1 + z + (pow(z,2)/2) + (pow(z,3)/3) + (pow(z,4)/4);

    cout<<pow(s1,s2)*s3;
    return 0;
}

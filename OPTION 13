#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    double x,y,z;

    cout<<"x ="<<'\n';
    cin>>x;

    cout<<"y ="<<'\n';
    cin>>y;

    cout<<"z ="<<'\n';
    cin>>z;

    double s0 = pow(y + pow(x-1,1/3.0), 1/4.0);
    double s1 = fabs(x-y)*((pow(sin(z),2) + tan(z)));
    
    cout<<s0/s1;
    return 0;
}

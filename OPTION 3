#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    double x,y,z;

    cout<<"x ="<<'\n';
    cin>>x;

    cout<<"y ="<<'\n';
    cin>>y;

    cout<<"z ="<<'\n';
    cin>>z;

    double s1 = 1 + pow(sin(x+y),2);
    double s2 = fabs((x - ((2*y)/(1.0 + (x * x)*(y*y)))));
    double s = s1 / s2 * pow(x,fabs(y)) + pow(cos(atan(1.0/z)),2);

    cout<<s;
    return 0;
}

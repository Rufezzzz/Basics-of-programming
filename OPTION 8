#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    double x,y,z;

    cout<<"x ="<<'\n';
    cin>>x;

    cout<<"y ="<<'\n';
    cin>>y;

    cout<<"z ="<<'\n';
    cin>>z;

    double s0 = exp(fabs(x-y));
    double s1 = fabs(x-y);
    double s2 = pow(s1,x+y);
    double s3 = atan(x) + atan(z);
    double s4 = pow(x,6) + pow(log(y),2);
    double s5 = pow(s4,1.0/3.0);

    cout<<s0*s2/s3+s5;
    return 0;
}

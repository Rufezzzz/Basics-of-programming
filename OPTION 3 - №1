#include <iostream>
#include <string>

using namespace std;

bool isAlphabeticalOrder(const string& str) {
    for (int i = 0; i < str.length() - 1; i++) {
        if (isalpha(str[i]) && isalpha(str[i+1])) { // Проверяем, что символы являются буквами
            if (tolower(str[i]) > tolower(str[i+1])) { // Если текущая буква больше следующей (приводим к нижнему регистру для сравнения)
                return false; // Набор не упорядочен по алфавиту
            }
        }
    }
    return true; // Набор упорядочен по алфавиту
}

int main() {
    string input;
    cout << "Введите строку: ";
    getline(cin, input);

    if (isAlphabeticalOrder(input)) {
        cout << "Набор букв упорядочен по алфавиту." << endl;
    } else {
        cout << "Набор букв не упорядочен по алфавиту." << endl;
    }

    return 0;
}

#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    setlocale(0,"");

    double a,b,c;

    cout << "Длина строки А = ";
    cin >> a;

    cout << "Длина строки B = ";
    cin >> b;

    cout << "Угол между сторонами А и В = ";
    cin >> c;

    if (a == b)
    {
        if (c == 90)
        {
            cout << "Этот параллелограмм является квадратом";
        }
        else
        {
            cout << "Этот параллелограмм является равносторонним ромбом";
        }
    }
    else
    {
        if (c == 90)
        {
            cout << "Этот параллелограмм является прямоугольником";
        }
        else
        {
            cout << "Этот параллелограмм является ромбом";
        }
    }

    return 0;
}

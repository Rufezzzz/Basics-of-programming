#include <iostream>

using namespace std;

int main()
{
    setlocale(0,"");

    int a, b, c, a1, b1, c1;

    cout << "Кол-во часов 1-го момента" << '\n';
    cin >> a;

    cout << "Кол-во минут 1-го момента" << '\n';
    cin >> b;

    cout << "Кол-во секунд 1-го момента" << '\n';
    cin >> c;

    cout << "Кол-во часов 2-го момента" << '\n';
    cin >> a1;

    cout << "Кол-во минут 2-го момента" << '\n';
    cin >> b1;

    cout << "Кол-во секунд 2-го момента" << '\n';
    cin >> c1;

    int s1 = a * 3600 + b * 60 + c;
    int s2 = a1 * 3600 + b1 * 60 + c1;

    cout << "Между 1-ым и 2-м моментами прошло " << s2 - s1 << " секунд";

    return 0;
}

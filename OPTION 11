#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    double x,y,z;

    cout<<"x ="<<'\n';
    cin>>x;

    cout<<"y ="<<'\n';
    cin>>y;

    cout<<"z ="<<'\n';
    cin>>z;

    double s0 = pow(y,pow(abs(x),1/3.0));
    double s1 = pow(cos(y),3);
    double s2 = (fabs(x-y)*(1 + ((pow(sin(z),2))/pow(x+y,1/2.0)))) / (exp(fabs(x-y)) + x/2.0);

    cout<<s0 + s1*s2;
    return 0;
}

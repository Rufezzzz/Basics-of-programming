#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    double x,y,z;

    cout<<"x ="<<'\n';
    cin>>x;

    cout<<"y ="<<'\n';
    cin>>y;

    cout<<"z ="<<'\n';
    cin>>z;

    double s0 = pow(x,1.0/3) + pow(x, y + 2);
    double s1 = pow(10 * s0, 1.0/2);
    double s2 = pow(asin(z), 2) - fabs(x-y);

    cout<<s1*s2;
    return 0;
}

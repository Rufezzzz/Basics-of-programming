#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    double x,y,z;

    cout<<"x ="<<'\n';
    cin>>x;

    cout<<"y ="<<'\n';
    cin>>y;

    cout<<"z ="<<'\n';
    cin>>z;

    double s0 = 5 * atan(x);
    double s1 = 1/4.0 * acos(x);
    double s2 = (x + 3 * fabs(x-y) + pow(x,2)) / (fabs(x-y) * z + pow(x,2));

    cout<<s0 - s1 * s2;
    return 0;
}

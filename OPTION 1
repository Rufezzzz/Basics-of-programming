#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    double x,y,z;

    cout<<"x = "<<'\n';
    cin>>x;

    cout<<"y = "<<'\n';
    cin>>y;

    cout<<"z = "<<'\n';
    cin>>z;

    double s1 = 2 * cos(x - 2.0/3.0);
    double s2 = 0.5 + pow(sin(y),2);
    double s3 = 1 + (z*z) / (3.0 - (z*z)/5.0);

    cout<<s1/s2*s3;

    return 0;
}
